{
	"opts": {
		"readme": "./OVERVIEW.md",
		"destination": "./jsdoc"
	},
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"source": {
		"include": ["src/lokijs.js", "src/loki-indexed-adapter.js"],
		"includePattern": ".+\\.js(doc)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": [],
	"templates": {
		"cleverlinks": false,
		"monospaceLinks": false
	}
}